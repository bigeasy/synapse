{ "name": "Synapse"
, "version": "0.0"
, "manifest_version": 2
, "description": "Creates a Synapse Peer on the Synapse network that runs from your Google Chrome browser."
, "permissions":
  [ "https://www.prettyrobots.com/synapse"
  ]
, "content_scripts":
  [
    { "matches": [ "http://synapse:8083/synapse" , "https://www.prettyrobots.com/synapse" ]
    , "js": [ "content.js" ]
    }
  ]
, "background": ["page", "background.html"]
}
